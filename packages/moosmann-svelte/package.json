{
    "name": "moosmann-svelte",
    "version": "0.6.1",
    "description": "moosmann for svelte",
    "type": "module",
    "keywords": [
        "moosmann",
        "i18n",
        "typescript",
        "svelte"
    ],
    "author": "Niphram",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/Niphram/moosmann"
    },
    "scripts": {
        "build": "tsup src/index.ts"
    },
    "dependencies": {
        "moosmann": "workspace:^"
    },
    "peerDependencies": {
        "svelte": "^3.0.0 || ^4.0.0 || ^5.0.0"
    },
    "files": [
        "dist",
        "package.json"
    ],
    "main": "src/index.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        }
    },
    "publishConfig": {
        "main": "dist/index.js"
    }
}
