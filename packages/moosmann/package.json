{
    "name": "moosmann",
    "version": "0.5.1",
    "description": "i18n for typescript developers",
    "keywords": [
        "i18n",
        "typescript"
    ],
    "author": "Niphram",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/Niphram/moosmann"
    },
    "scripts": {
        "build": "tsup src/index.ts"
    },
    "files": [
        "dist",
        "package.json"
    ],
    "main": "src/index.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        }
    },
    "publishConfig": {
        "main": "dist/index.js"
    }
}
